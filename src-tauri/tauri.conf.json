{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mural",
  "version": "0.1.0",
  "identifier": "com.mural.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
  "label": "main",
  "title": "mural",
        "width": 1200,
        "height": 800,
        "decorations": false,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["rpm"],
    "icon": [
      "icons/mural/web/icon-192.png",
      "icons/mural/web/icon-512.png"
    ]
  },
  "plugins": {
    "fs": {}
  }
}
